{
  "name": "insurance-feedback-system",
  "version": "1.0.0",
  "description": "Professional lead management and feedback system for insurance lead generation business",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run db:migrate && npm run db:seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "npx playwright install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "knex": "^3.0.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "uuid": "^9.0.1",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "prom-client": "^15.1.0",
    "@sendgrid/mail": "^8.1.0",
    "aws-sdk": "^2.1498.0",
    "redis": "^4.6.10",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "eslint-config-node": "^4.1.0",
    "eslint-plugin-node": "^11.1.0",
    "playwright": "^1.40.1",
    "@types/jest": "^29.5.8"
  },
  "engines": {
    "node": "18.x",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "insurance",
    "lead-generation",
    "feedback",
    "crm",
    "postgresql",
    "express",
    "nodejs"
  ],
  "author": "Insurance Lead Generation Business",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourcompany/insurance-feedback-system.git"
  },
  "bugs": {
    "url": "https://github.com/yourcompany/insurance-feedback-system/issues"
  },
  "homepage": "https://github.com/yourcompany/insurance-feedback-system#readme"
}